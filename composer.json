{
  "name": "mtrdesign/s3-analytics",
  "type": "library",
  "description": "Analytics for Amazon S3",
  "keywords": [
    "s3",
    "analytics"
  ],
  "homepage": "https://mtr-design.com",
  "support": {
    "issues": "https://github.com/mtrdesign/s3-analytics/issues",
    "source": "https://github.com/mtrdesign/s3-analytics"
  },
  "authors": [
    {
      "name": "MTR Design",
      "email": "office@mtr-design.com",
      "homepage": "https://mtr-design.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1.8",
    "ext-json": "*",
    "aws/aws-sdk-php": "^3.100",
    "nesbot/carbon": "^2.19"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "S3Analytics\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ],
    "phpcs": "phpcs -p --standard=PSR2 src tests",
    "phpstan": "phpstan analyse --level 7 src",
    "phpunit": "phpunit --coverage-clover=coverage.xml --verbose"
  }
}
